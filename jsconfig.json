{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@validation/*": [
                "src/validation/*"
            ],
            "@middleware/*": [
                "src/middleware/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@helpers/*": [
                "src/helpers/*"
            ],
            "@configs/*": [
                "src/configs/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@contexts/*": [
                "src/contexts/*"
            ],
            "@src/*": [
                "src/*"
            ]
        }
    }
}